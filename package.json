    "jsdom": "^13.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-uglify": "^6.0.2",
    "sleep-promise": "^8.0.1",
    "standard": "^12.0.1",
    "tap-spec": "^5.0.0",
    "tape": "^4.9.2"
  },
  "standard": {
    "parser": "babel-eslint"
  }
